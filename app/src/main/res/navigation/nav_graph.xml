<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splash">
    <fragment
        android:id="@+id/home2"
        android:name="com.example.picobotellaequipo6.view.fragment.home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home2_to_instructions"
            app:destination="@id/instructions" />
        <action
            android:id="@+id/action_home2_to_challenges2"
            app:destination="@id/challenges2" />
    </fragment>
    <fragment
        android:id="@+id/instructions"
        android:name="com.example.picobotellaequipo6.view.fragment.instructions"
        android:label="fragment_instructions"
        tools:layout="@layout/fragment_instructions" />
    <fragment
        android:id="@+id/challenges2"
        android:name="com.example.picobotellaequipo6.view.fragment.challenges"
        android:label="fragment_challenges"
        tools:layout="@layout/fragment_challenges" />
    <fragment
        android:id="@+id/splash"
        android:name="com.example.picobotellaequipo6.view.fragment.Splash"
        android:label="Splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splash_to_home2"
            app:destination="@id/home2" />
    </fragment>
</navigation>